{
  "name": "frets-starter",
  "version": "1.0.0",
  "description": "frets-starter-project",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": ""
  },
  "scripts": {
    "start": "npm run server:dev",
    "server:debug": "cross-env NODE_ENV=development node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js -d ",
    "server:dev": "cross-env NODE_ENV=development node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js -d",
    "server:prod": "cross-env NODE_ENV=production node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js -p",
    "build:dev": "cross-env NODE_ENV=development webpack -d",
    "build:prod": "cross-env NODE_ENV=production webpack -p && npm run css && ./node_modules/.bin/tsc -p src/server/",
    "test": "jest",
    "css": "postcss src/app.css -c postcss.config.js -o dist/app.css",
    "base": "frets-styles-generator src/",
    "base:watch": "frets-styles-generator src/ -w",
    "minify": "postcss dist/app.css -u cssnano -o dist/app.min.css"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/test/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleDirectories": [
      "node_modules",
      "bower_components",
      "shared"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.json"
      }
    }
  },
  "cssnano": {
    "preset": [
      "default",
      {
        "discardComments": {
          "removeAll": true
        }
      }
    ]
  },
  "author": "Tim Bendt",
  "license": "MIT",
  "devDependencies": {
    "@types/babel-core": "^6.25.2",
    "@types/html-webpack-plugin": "^2.28.0",
    "@types/jest": "^21.1.2",
    "@types/moment": "^2.13.0",
    "@types/optimize-css-assets-webpack-plugin": "^1.3.1",
    "@types/webpack": "^3.0.13",
    "@types/webpack-bundle-analyzer": "^2.9.0",
    "@types/webpack-dev-server": "^2.9.0",
    "autoprefixer": "^6.3.1",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "colors.css": "^3.0.0",
    "commander": "^2.12.2",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "cssnano": "^3.4.0",
    "extract-text-webpack-plugin": "^3.0.1",
    "file-loader": "^1.1.5",
    "frets": "^0.2.7",
    "frets-styles-generator": "^0.1.5",
    "html-webpack-plugin": "^2.28.0",
    "ignore-loader": "^0.1.2",
    "jest": "^21.2.1",
    "maquette-query": "^0.11.7",
    "moment-locales-webpack-plugin": "^1.0.3",
    "moment-round": "^1.0.1",
    "netlify-lambda": "^0.3.1",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "postcss": "^6.0.13",
    "postcss-calc": "^6.0.1",
    "postcss-cli": "^4.1.1",
    "postcss-color-function": "^4.0.0",
    "postcss-custom-media": "^6.0.0",
    "postcss-custom-properties": "^6.2.0",
    "postcss-discard-comments": "^2.0.3",
    "postcss-fontpath": "^1.0.0",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.0.8",
    "ramda": "^0.25.0",
    "source-map-loader": "^0.2.2",
    "style-loader": "^0.19.0",
    "ts-jest": "^21.1.2",
    "ts-loader": "2.3.7",
    "typescript": "^2.5.3",
    "webpack": "^3.7.1",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-dev-server": "^2.9.1"
  },
  "dependencies": {
    "axios": "^0.17.0",
    "basscss": "^8.0.0",
    "basscss-addons": "^1.0.0-beta4",
    "basscss-basic": "^1.0.0",
    "camel-case": "^3.0.0",
    "change-case": "^3.0.1",
    "maquette": "^3.2.0",
    "moment": "^2.20.1",
    "tachyons": "^4.11.1"
  }
}
