{
  "name": "frets-starter",
  "version": "1.0.0",
  "description": "frets-starter-project",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": ""
  },
  "scripts": {
    "start": "npm run server:dev",
    "server:debug": "cross-env NODE_ENV=development webpack-dev-server --mode=development",
    "server:dev": "cross-env NODE_ENV=development webpack-dev-server --mode=development",
    "server:prod": "cross-env NODE_ENV=production webpack-dev-server",
    "build:dev": "cross-env NODE_ENV=development webpack -d",
    "build:prod": "cross-env NODE_ENV=production webpack -p && npm run css",
    "test": "jest",
    "css": "postcss src/app.css -c postcss.config.js -o dist/app.css",
    "base": "frets-styles-generator src/",
    "base:watch": "frets-styles-generator src/ -w",
    "minify": "postcss dist/app.css -u cssnano -o dist/app.min.css"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/test/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleDirectories": [
      "node_modules",
      "bower_components",
      "shared"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.json"
      }
    }
  },
  "cssnano": {
    "preset": [
      "default",
      {
        "discardComments": {
          "removeAll": true
        }
      }
    ]
  },
  "author": "Tim Bendt",
  "license": "MIT",
  "devDependencies": {
    "@types/html-webpack-plugin": "^3.2.0",
    "@types/jest": "^23.3.2",
    "@types/moment": "^2.13.0",
    "@types/optimize-css-assets-webpack-plugin": "^1.3.3",
    "@types/webpack": "^4.4.12",
    "@types/webpack-bundle-analyzer": "^2.13.0",
    "@types/webpack-dev-server": "^3.1.1",
    "autoprefixer": "^9.1.5",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "cssnano": "^4.1.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^2.0.0",
    "frets": "^0.3.0",
    "frets-styles-generator": "^0.1.6",
    "html-webpack-plugin": "^3.2.0",
    "ignore-loader": "^0.1.2",
    "jest": "^23.6.0",
    "maquette-query": "^0.11.7",
    "moment-locales-webpack-plugin": "^1.0.7",
    "moment-round": "^1.0.1",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss": "^7.0.2",
    "postcss-cli": "^6.0.0",
    "postcss-preset-env": "^6.0.1",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.0",
    "ts-jest": "^23.10.1",
    "ts-loader": "5.1.1",
    "typescript": "^3.0.3",
    "webpack": "^4.19.1",
    "webpack-bundle-analyzer": "^3.0.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.8"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "maquette": "^3.3.1",
    "moment": "^2.22.2",
    "mongodb-stitch-browser-sdk": "^4.1.0",
    "tachyons": "^4.11.1"
  }
}
